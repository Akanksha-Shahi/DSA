// Left rotate an array by 1 place
#include <iostream>
using namespace std;
int main(){
    int a[] = {1, 5, 3, 9, 2, 8, 6, 4, 7};
    int n = sizeof(a)/sizeof(a[0]);
    int temp = a[0];
    for (int i=1; i<n; i++){
        a[i-1] = a[i];
    }
    a[n-1] = temp;
    
    for (int i=0; i<n; i++){
        cout<< a[i]<<" ";
    }
    return 0;
}
