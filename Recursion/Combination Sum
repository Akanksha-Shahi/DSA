#include <iostream>
#include <vector>
using namespace std;
void combinationSumUtil(int ind, int target, vector<int> &arr, vector<vector<int>> &ans, vector<int> &ds){
    // base case
    if (ind == arr.size()){
        if (target == 0){
            ans.push_back(ds);
        }
        return;
    }
    // take or include the element
    if (arr[ind] <= target){
        ds.push_back(arr[ind]);
        combinationSumUtil(ind, target - arr[ind], arr, ans, ds);
        ds.pop_back();
    }
    // not take or exclude the element
    combinationSumUtil(ind +1, target, arr, ans, ds);
}
vector<vector<int>> combinationSum(vector<int> &arr, int target){
    vector<vector<int>> ans;
    vector<int> ds;
    combinationSumUtil(0, target, arr, ans, ds);
    return ans;
}
int main(){
    vector<int> arr= {2,3,6,7};
    int target=7;
    vector<vector<int>> ans= combinationSum(arr, target);
    for (auto it: ans){
        for (auto i: it){
            cout<< i<< " ";
        }
        cout<< endl;
    }
    return 0;
}
