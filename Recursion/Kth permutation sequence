#include <iostream>
#include <vector>
#include <string>
using namespace std;
string getPrm( int n , int k ){
    int fact = 1;
    vector<int> number;
    for ( int i=1; i< n; i++){
        fact= fact * i;
        number.push_back(i);
    }
    number.push_back(n);
    string ans= "";
    k= k -1; // zero based indexing
    while ( true ){
        ans += to_string( number[k/ fact]);
        number.erase( number.begin() + k/ fact);
        if ( number.size() ==0){
            break;
        }
        k= k % fact;
        fact= fact/ number.size();
    }
    return ans;
}
int main(){
    int n=4;
    int k=9;
    string ans= getPrm(n, k);
    cout<< ans<< endl;
    return 0;
}
