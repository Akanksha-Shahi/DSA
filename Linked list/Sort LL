#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
class Node {
    public:
    int data;
    Node* next;
    Node* prev;
    Node* child;

    public:
    Node( int data1, Node* next1, Node* prev1, Node* child1) {         // constructor
        data = data1;
        next = next1;
        prev = prev1;
        child = child1;
    }

    public:
    Node (int data1) {         // constructor
        data = data1;
        next = nullptr;
        prev = nullptr;
        child = nullptr;
    }
};
// brute: put in array and sort
Node* sortLL( Node* head){
    if ( head == NULL || head->next == NULL) return head;
    vector<int> arr;
    Node* temp = head;
    while( temp != NULL){
        arr.push_back( temp->data);
        temp = temp->next;
    }
    sort( arr.begin(), arr.end());
    int i= 0;
    temp = head;
    while( temp != NULL){
        temp->data = arr[i];
        i++;
        temp = temp->next;
    }
    return head;
};
