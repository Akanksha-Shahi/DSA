#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
class Node {
    public:
    int data;
    Node* next;
    Node* prev;

    public:
    Node( int data1, Node* next1, Node* prev1) {         // constructor
        data = data1;
        next = next1;
        prev = prev1;
    }

    public:
    Node (int data1) {         // constructor
        data = data1;
        next = nullptr;
        prev = nullptr;
    }
};
// brute: traverse through all ele of L1 and L2 and put it in an array
Node* convertArrayToLinkedList( vector<int> &arr){
    Node* head = new Node(arr[0]);
    Node* mover = head;
    for (int i = 1; i<arr.size(); i++){
        Node* temp = new Node(arr[i]);
        mover->next = temp;         // linking node to node
        mover = mover->next;        // moving the mover to next node ( move to temp )
    }
    return head;
}

Node* mergeTwoSortedLL( Node* head1, Node* head2){
    vector<int> arr;
    if ( head1 == NULL) return head2;
    if ( head2 == NULL) return head1;
    Node* temp1 = head1;
    Node* temp2 = head2;
    while( temp1 != NULL){
        arr.push_back( temp1->data);
        temp1 = temp1->next;
    }
    while( temp2 != NULL){
        arr.push_back( temp2->data);
        temp2 = temp2->next;
    }
    sort( arr.begin(), arr.end());
    Node* head = convertArrayToLinkedList( arr);
    return head;
};
