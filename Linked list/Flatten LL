#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
class Node {
    public:
    int data;
    Node* next;
    Node* prev;
    Node* child;

    public:
    Node( int data1, Node* next1, Node* prev1, Node* child1) {         // constructor
        data = data1;
        next = next1;
        prev = prev1;
        child = child1;
    }

    public:
    Node (int data1) {         // constructor
        data = data1;
        next = nullptr;
        prev = nullptr;
        child = nullptr;
    }
};
// brute: take all integers and put it in an array 
Node* convert( vector<int>& arr){
    if ( arr.size() == 0) return NULL;
    Node* head = new Node( arr[0]);
    Node* temp = head;
    for( int i = 1; i < arr.size(); i++){
        Node* newNode = new Node( arr[i]);
        temp->child = newNode;
        temp = temp->child;
    }
    return head;
} ;
Node* flattenLL( Node* head ){
    if ( head == NULL) return NULL;
    vector<int> arr;
    Node* temp = head;
    while( temp != NULL){
        Node* downNode = temp;
        while( downNode != NULL){
            arr.push_back( downNode->data);
            downNode = downNode->child;
        }
        temp = temp->next;
    }
    sort( arr.begin(), arr.end());
    Node* head = convert(arr);    
} ;
