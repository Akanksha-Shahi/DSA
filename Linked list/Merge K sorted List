#include <iostream>
#include <vector>
#include <queue>
#include <algorithm>
using namespace std;
class Node {
    public:
    int data;
    Node* next;
    Node* prev;
    Node* child;

    public:
    Node( int data1, Node* next1, Node* prev1, Node* child1) {         // constructor
        data = data1;
        next = next1;
        prev = prev1;
        child = child1;
    }

    public:
    Node (int data1) {         // constructor
        data = data1;
        next = nullptr;
        prev = nullptr;
        child = nullptr;
    }
};

// brute: merge two sorted LL repeatedly and store in an array / list
Node* mergeTwoSortedLLBrute( vector<Node*> &lists, Node* head){
    vector<int> arr;
    for ( auto head : lists){
        Node* temp = head;
        while( temp != NULL){
            arr.push_back( temp->data);
            temp = temp->next;
        }
    }
    sort( arr.begin(), arr.end());
    head = convertArr2LL(arr);
    return head;
}
