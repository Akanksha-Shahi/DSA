#include<bits/stdc++.h>
using namespace std;
int func( vector<int> &nums, int goal){
    if ( goal<0) return 0;
    int l=0, r=0, count =0;
    while( r<= nums.size()){
        sum += nums[r];
        while( sum> goal){
            sum -= nums[l];
            l++;
        }
        count += (r-l+1);
        r++;
    }
    return count;
}
int main(){
    vector<int> nums = {1,0,1,0,1};
    int goal =2;
    cout << func( nums, goal) << endl;
    return 0;
}
