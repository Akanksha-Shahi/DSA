#include <bits/stdc++.h>
using namespace std;
// brute: check each person
int func(matrix<int> &arr, int n ){
    vector<int> know(n,0);
    vector<int> iknow(n,0);
    for( int i =0; i<n; i++){
        for( int j =0; j<n; j++){
            if(arr[i][j]== 1){
                know[i]++;
                iknow[j]++;
            }
        }
    }
    for( int i =0; i<n; i++){
        if(know[i]==n-1 && iknow[i]==0) return i;
    }
    return -1;
}
