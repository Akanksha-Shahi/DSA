#include<bits/stdc++.h>
using namespace std;
// brute:
vector<int> nearSmallerr(vector<int> &arr, int n){
    vector<int> ans(n);
    for( int i =0; i<n; i++){
        for( int j = i-1; j>=0; j--){
            if( arr[j]< arr[i]){
                ans[i] = arr[j];
                break;
            }
        }
    }
    return ans;
}
// optimal: use of stack
list<int> nearSmall(vector<int> &arr, int n){
    vector<int> ans(n);
    stack<int> st;
    for( int i =0; i<n; i++){
        while( !st.empty() && st.top() >= arr[i]){
            st.pop();
        }
        if( st.empty()){
            ans[i] = -1;
        }
        else{
            ans[i] = st.top();
        }
        st.push( arr[i]);
    }
    return ans;
}
int main(){
    int n; cin>> n;
    vector<int> arr(n);
    for( int i =0; i<n; i++){
        cin>> arr[i];
    }
    vector<int> res = nearSmall( arr, n);
    for( auto x: res){
        cout<< x<< " ";
    }
    return 0;
}
