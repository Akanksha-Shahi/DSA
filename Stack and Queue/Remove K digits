#include <bits/stdc++.h>
using namespace std;
string func(string s, int k) {
    stack<char> st;
    int n = s.size();
    // Remove digits
    for(int i = 0; i < n; i++) {
        while(!st.empty() && k > 0 && st.top() > s[i]) {
            st.pop();
            k--;
        }
        st.push(s[i]);
    }
    // If k still remains
    while(k > 0 && !st.empty()) {
        st.pop();
        k--;
    }
    // Build result
    string res = "";
    while(!st.empty()) {
        res += st.top();
        st.pop();
    }
    reverse(res.begin(), res.end());
    // Remove leading zeros
    while(res.size() > 0 && res[0] == '0') {
        res.erase(res.begin());
    }
    if(res.empty())
        return "0";
    return res;
}
