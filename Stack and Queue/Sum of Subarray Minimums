#include<bits/stdc++.h>
using namespace std;
// brute:
int sumSub( vector<int> &arr, int n){
    int sum =0;
    for( in ti =0; i< n; i++){
        int mini = arr[i];
        for( j = i ; j<n; j++){
            mini= min( mini, arr[j]);
            sum =(sum +mini)%1000000007;
        }
    }
    return sum;
}
