// Stack using Array 
#include<bits/stdc++.h>
using namespace std;
class Stack{
    public: 
    int top =-1;
    int size[10];
    void push( int x){
        if( top>=10){
            top++;
            size[top]=x;
        }
    }
    void pop(){
        if( top == -1) return;
        top--;
    }
    int top(){
        if( top == -1) return;
        return size[top];
    }
    int size(){
        return top+1;
    }
}

// Queue using array
#include<bits/stdc++.h>
using namespace std;
class Queue{
    public:
    int size =10;
    int start, end =-1;
    int q[10];
    int curSize =0;
    void push( int x){
        if( curSize == size) return;
        if( curSize ==0) start = end =0;
        else end  =( end+1)% size;
        q[end]=x;
        curSize++;
    }
    void pop(){
       if( curSize == 0) return;  
       if( curSize ==1) start = end = -1;
       else start = ( start+1) % size;
       cursize--;
       return;
    }
    int top(){
        if( curSize ==0) return -1;
        return q[start];
    }
    int size(){
        return curSize;
    }
}

// Stack using Linked List
#include<bits/stdc++.h>
using namespace std;
class Node{
    public:
    int data;
    Node* next;
    Node( int val){
        data = val;
        next = NULL;
    };
};
class Stack{
    public:
    Node * top;
    int size =0;
    void push( int x){
        Node* newNode = new Node(x);
        newNode->next = top;
        top = newNode;
        size++;
    }
    void pop(){
        if( top == NULL) return;
        Node* temp = top;
        top = top->next;
        delete temp;
        size--;
    }
    int topValue(){
        if( top == NULL) return -1;
        return top->data;
    }
    int sizeValue(){
        return size;
    }
}

// Queue using Linked List
#include<bits/stdc++.h>
using namespace std;
class Node{
    public:
    int data;
    Node* next;
    Node( int val){
        data = val;
        next = NULL;
    };
};
class Queue{
    public:
    Node* start, end;
    int size =0;
    void push( int x){
        Node newnode = new Node(x);
        if( start == NULL) start = end= newnode;
        else{
            end->next = newnode;
            end = newnode;
        }
    }
    void pop(){
        if( start == NULL) return;
        Node* temp = start;
        start = start->next;
        delete temp;
        size--;
    }
    int top(){
        if( start == NULL) return -1;
        return start->data;
    }

// Stack using one queue
#include<bits/stdc++.h>
using namespace std;
class Stack{
    public:
    queue<int> q1;
    void push( int x){
        q1.push(x);
        int sz = q1.size();
        for( int i =0; i< sz-1; i++){
            q1.push( q1.front());
            q1.pop();
        }
    }
    void pop(){
        if( q1.empty()) return;
        q1.pop();
    }
    int top(){
        if( q1.empty()) return -1;
        return q1.front();
    }
    int size(){
        return q1.size();
    }
};

// Queue using two stack
#include<bits/stdc++.h>
using namespace std;
class Queue{
    public:
    stack<int> s1, s2;
    void push( int x){
        s1.push(x);
    }
    void pop(){
        if( s2.empty()){
            while( !s1.empty()){
                s2.push( s1.top());
                s1.pop();
            }
        }
        if( !s2.empty()){
            s2.pop();
        }
    }
    int top(){
        if( s2.empty()){
            while( !s1.empty()){
                s2.push( s1.top());
                s1.pop();
            }
        }
        if( !s2.empty()){
            return s2.top();
        }
        return -1;
    }
    int size(){
        return s1.size() + s2.size();
    }
};
    int size(){
        return size;
    }
}
