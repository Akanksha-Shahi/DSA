// MAP
// Unorderd Map :- here one cannot decide the order
#include <iostream>
#include <map> // for map
#include <unordered_map> // for unordered_map
using namespace std;
int main() {
    int n; cin>> n ;
    int arr[n];
    for (int i = 0; i < n; i++) {
        cin>> arr[i];
    }
    // precompute
    map<int, int> mpp; // key, value pair
    for (int i = 0; i < n; i++) {
        mpp[arr[i]]++;
    }
    for (auto it : mpp) {                   // iterate through the map
        cout << it.first << "->" << it.second << endl; // print key and value
    }
    int q; cin>> q;
    while (q--) {
        int number; cin>> number;
        cout<< mpp[number] << endl; // fetch
    }
    return 0;}
