#include <bits/stdc++.h>
using namespace std;
struct Node{
    Node* links[26];
    bool containsKey( char ch){
        return ( links[ch - 'a'] != NULL);
    }
    void put( char ch, Node* node){
        links[ch - 'a'] = node;
    }    
    Node* get( char ch){
        return links[ch -'a'];
    }
};    
int countDis( string &s){
    int cnt =0;
    Node* root = new Node();
    for( int count =0; i<word.size(); i++){
        Node* node = root;
        for( int j =i; j<word.size(); j++){
            char ch = word[i];
            if( !node->containsKey(ch)){
                node->put(ch, new Node());
                cnt++;
            }
            node = node->get(ch);
        }
    }
    return cnt+1;
}
