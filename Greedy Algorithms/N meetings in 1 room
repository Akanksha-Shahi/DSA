#include<bits/stdc++.h>
using namespace std;
struct Data{
    int start, end, n;
};
bool compare( const Data a, const Data b){
    return a.end  < b.end;
};
int func( vector<int> &start, vector<int> &end, int n){
    vector<Data> arr(n);
    for( int i =0; i<n; i++){
        arr[i].start = start[i];
        arr[i].end = end[i];
        arr[i].n = i+1;
    }
    sort ( arr.begin(), arr.end(), compare);
    int count =1, freeTime = arr[0].end;
    vector<int> ds;
    ds.push_back(arr[0].n);
    for( int i =1; i<n; i++){
        if( arr[i].start >= freeTime){
                count++;
                freeTime = arr[i].end;
                ds.push_back( arr[i].n);
            }
        }
    return count;
    }


int main(){
    vector<int> start = {1,3,0,5,8,5};
    vector<int> end = {2,4,6,7,9,9};
    cout << func(start, end, start.size()) << endl;
    return 0;
}
