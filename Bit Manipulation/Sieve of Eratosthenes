// Sieve of Eratosthenes: Print all prime numbers up to n
#include<bits/stdc++.h>
using namespace std;
int func( int n ){
    vector<int> prime[n+1];
    for( int i =2; i<=n; i++) prime[i]=1;
    for( int i =2; i*i <= n; i++){
        if( prime[i] == 1){
            for( int j = 2*i; j<= n; j+=i){
                prime[j] =0;
            }
        }
    }
    for( int i =2; i<= n; i++){
        if( prime[i] ==1) cout << i << " ";
    }
}
int main(){
    int n =50;
    func( n);
    cout << endl;
    return 0;
}
