// Smallest prime factor
#include<bits/stdc++.h>
using namespace std;
vector<int> spf( int n){
    vector<int> spf(n+1);
    for( int i =1; i<= n; i++) spf[i] = i;
    for( int i =2; i*i <= n; i++){
        if( spf[i] == i){
            for( int j = i*i; j<= n; j+=i){
                if( spf[j] == j){
                    spf[j] = i;
                }
            }
        }
    }
    return spf;
}
int main(){
    int n =50;
    vector<int> ans = spf( n);
    for( int i =2; i<= n; i++){
        cout << i << " -> " << ans[i] << endl;
    }
    return 0;
}
