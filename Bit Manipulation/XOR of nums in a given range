// XOR of numbers in a  given range
#include<bits/stdc++.h>
using namespace std;
// brute: simple iteration
int inrange( int n){
    int xor =0;
    for( int i =0; i<=n; i++){
        xor^=i;
    }
    return xor;
}
// optimal: pattern observation
int func(int n ){
    if( n%4 ==1) return 1;
    else if( n%4 ==2) return n+1;
    else if( n%4 == 3) return 0;
    else return n;
}
int inrangeOpt( int a, int b){
    return func(b) ^ func(a-1);
}
int main(){
    int a =3, b=9;
    cout << inrange( b) ^ inrange( a-1) << endl;
    cout << inrangeOpt( a, b) << endl;
    return 0;
}
