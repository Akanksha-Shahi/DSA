// Power Exponential
#include<bits/stdc++.h>
using namespace std;
long long power( long long x, long long n){
    long long ans =1;
    while(n>0){
        if( n%2 ==1){
            ans *= x;
            n--;
        }
        else{
            x*=x;
            n/=2;
        }
    }
    return ans;
}
int main(){
    long long x =2;
    long long n =10;
    cout << power( x, n) << endl;
    return 0;
}
