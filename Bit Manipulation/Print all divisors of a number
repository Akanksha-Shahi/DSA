#include<bits/stdc++.h>
using namespace std;
// brute: simple iteration
vector<int> divisors(int n){
    vector<int> list;
    for( int i =0; i<=n; i++){
        if( n%i ==0) list.push_back(i);
    }
    return list;
}
// optimal: using pair property
vector<int> divisor1( int n){
    vector<int> list;
    for( int i=1; i<=sqrt(n); i++){
        if( n%i ==0 ){
            list.push_back(i);
            if( n/i != i ) list.push_back(n/i);
        }
    }
    sort( list.begin(), list.end());
    return list;
}
int main(){
    int n =36;
    vector<int> ans = divisor1( n);
    for( auto it: ans){
        cout << it << " ";
    }
    cout << endl;
    return 0;
}
