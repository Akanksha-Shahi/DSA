#include<bits/stdc++.h>
using namespace std;
// brute
int prime( int n){
    int count =0;
    for (int i = 0; i < sqrt(n); i++)
    {
        if( n%i ==0){
            count++;
            if( n/i != i) count++; 
        }
        if( count > 2 ) break;
    }
    if( count ==2) return 1;;
    return 0;    
}
vector<int> primeFactors( int n){
    vector<int> list;
    for( int i =2; i<= n; i++){
        if( n%i ==0 && prime(i)){
            list.push_back(i);
        }
    }
    return list;
}
